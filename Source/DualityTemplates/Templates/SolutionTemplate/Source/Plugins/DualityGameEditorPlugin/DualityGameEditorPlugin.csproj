<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>DualityGameEditorPlugin.editor</AssemblyName>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="AdamsLair.Duality" Version="4.0.0-alpha3" />
    <PackageReference Include="AdamsLair.Duality.Primitives" Version="4.0.0-alpha3" />
    <PackageReference Include="AdamsLair.Duality.Editor" Version="4.0.0-alpha3" />
  </ItemGroup>

</Project>
